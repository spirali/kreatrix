
import: "test".

floatTest = test TestCase clone do: {
	testCompare = {
		(1.0 == 1.0) assertTrue.
		(1.0 == 2.0) assertFalse.

		(1.0 != 1.0) assertFalse.
		(1.0 != 2.0) assertTrue.

	}.
	
	testCopy = {
		assert: 3.1415 copy equals: 3.1415.
	}.

	testLiterals = {
		(0.1e10 > 0.2) assertTrue.
		(0.2e-10 < 0.1) assertTrue.
	}

}.

floatTest run.

